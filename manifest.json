{
	"name": "NewT",
	"version": "1.3.0",
	"manifest_version": 2,
	"description": "NewT(ab), a totally free and open source new tab page built for Chrome!",
	"homepage_url": "https://momentumdash.com",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
    "permissions": [
          "storage",
		  "identity",
		  "https://varvy.com/*"
    ],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
